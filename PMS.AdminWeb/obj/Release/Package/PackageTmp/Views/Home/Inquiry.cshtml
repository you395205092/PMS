@model ExamDTO
@{
    Layout = null;
    ViewBag.Title = "Inquiry";
}
<script language="javascript">
        function doPrint() {
            bdhtml = window.document.body.innerHTML;
            sprnstr = "<!--startprint-->"; //开始打印标识字符串有17个字符
            eprnstr = "<!--endprint-->"; //结束打印标识字符串
            prnhtml = bdhtml.substr(bdhtml.indexOf(sprnstr) + 17); //从开始打印标识之后的内容
            prnhtml = prnhtml.substring(0, prnhtml.indexOf(eprnstr)); //截取开始标识s和结束标识之间的内容
            window.document.body.innerHTML = prnhtml; //把需要打印的指定内容赋给body.innerHTML
            window.print(); //调用浏览器的打印功能打印指定区域
        }
</script>
<div style=" width:880px; margin:0 auto; line-height:20px;">
    <table width="750" border="0" align="center" cellpadding="0" cellspacing="0">
        <tr>
            <td height="40" align="right" style="line-height:20px;"><a href="javascript:void(0);" onClick="doPrint()" style="color:#000000;">【打印该页】</a>  <a href="/" style="color:#000000;">【返回首页】</a><br /></td>
        </tr>
    </table>
</div>
 <!--startprint-->
    <table width="460" border="0" align="center" cellpadding="0" cellspacing="0">
    <tr>
        <td>
            <table width="450" border="0" align="center" cellpadding="0" cellspacing="0" style="font-size:12px;">
                <tr>
                    <td colspan="2" align="center" style="font-size:16px; font-weight:bold; padding-bottom:10px;">@Model.TypeName<br />准考证</td>
                    <td width="117" rowspan="5" align="right"><img src="/uploads/pic/@(Model.StuId).jpg" width="106" height="139" /></td>
                </tr>
                <tr>
                    <td width="98" height="24" style="font-weight:bold;">准考证号：</td>
                    <td width="235" style="border-bottom:2px solid #333;">@Model.ZKZCode</td>
                </tr>
                <tr>
                    <td height="24" style="font-weight:bold;">学号：</td>
                    <td style="border-bottom:2px solid #333;">@Model.StuId</td>
                </tr>
                <tr>
                    <td height="24" style="font-weight:bold;">考生姓名：</td>
                    <td style="border-bottom:2px solid #333;">@Model.StuName</td>
                </tr>
                <tr>
                    <td height="24" style="font-weight:bold;">证件号码：</td>
                    <td style="border-bottom:2px solid #333;">@Model.SFZCode</td>
                </tr>
                <tr>
                    <td height="24" style="font-weight:bold;">上机考试地点：</td>
                    <td style="border-bottom:2px solid #333;">@Model.LLExamAddress</td>
                    <td align="center" style="border-bottom:2px solid #333;">上机座位号：@Model.LLExamPlaceNum</td>
                </tr>
                <tr>
                    <td height="24" style="font-weight:bold;">上机考试时间：</td>
                    <td colspan="2" style="border-bottom:2px solid #333;">@Model.LLExamTime</td>
                </tr>
                <tr>
                    <td height="24" style="font-weight:bold;">理论考试地址：</td>
                    <td  style="border-bottom:2px solid #333;">@Model.Address</td>
                    <td align="center" style="border-bottom:2px solid #333;">考场号：@Model.PlaceNum </td>
                </tr>
                <tr>
                    <td height="24" style="font-weight:bold;">理论考试时间：</td>
                    <td style="border-bottom:2px solid #333;">@Model.ExamTime</td>
                    <td align="center" style="border-bottom:2px solid #333;">座位号：@Model.llzwh</td>
                    
                </tr>
                <tr>
                    <td height="24" style="font-weight:bold;">学校名称：</td>
                    <td colspan="2" style="border-bottom:2px solid #333;">@Model.SchoolName</td>
                </tr>
                <tr>
                    <td height="24" style="font-weight:bold;">学院系别：</td>
                    <td colspan="2" style="border-bottom:2px solid #333;">@Model.SysName   @Model.MajorName  @Model.ClassName</td>
                </tr>
                <tr>
                    <td height="24" style="font-weight:bold;">备注：</td>
                    <td colspan="2" style="padding-top:10px;">@Model.TypeDescription</td>
                </tr>
            </table>

        </td>
    </tr>
</table>
<!--endprint-->

