@{
    ViewBag.Title = "批量更新为未查询";
}

<article class="page-container">
    <form class="form form-horizontal" action="/Exam/UpdateFind/" method="post" id="formAdd">

        <div class="row cl">
            <label class="form-label col-xs-4 col-sm-3"><span class="c-red">*</span>考试类别：</label>
            <div class="formControls col-xs-8 col-sm-9">
                <select id="TypeId" name="TypeId">
                    @foreach (var item in Model)
                    {
                        <option value="@item.Id">@item.Name</option>
                    }
                </select>
            </div>
        </div>
        <div class="row cl">
            <div class="col-xs-8 col-sm-9 col-xs-offset-4 col-sm-offset-3">
                <input class="btn btn-primary radius" type="submit" id="btnSave" value="&nbsp;&nbsp;提交&nbsp;&nbsp;">
            </div>
        </div>
    </form>
</article>
